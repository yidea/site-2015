{
  "name": "site-2015",
  "version": "0.0.1",
  "description": "Site 2015",
  "scripts": {
    "lint-client": "eslint --ext .js,.jsx --rule 'no-extra-parens: 0' client",
    "lint-server": "eslint --env node --rule 'no-var: 0' server *.js",
    "lint": "npm run lint-server && npm run lint-client",
    "test": "npm run lint",
    "start": "node server/index.js",
    "server": "nodemon --watch client --watch server --ext js,jsx server/index.js",
    "server-dev": "webpack-dev-server --config webpack.config.dev.js --progress --colors --port 2992",
    "server-hot": "webpack-dev-server --config webpack.config.hot.js --hot --progress --colors --port 2992 --inline",
    "sources": "http-server -p 3001 .",
    "watch": "webpack --watch",
    "prod": "npm run watch & npm run server & npm run sources",
    "dev": "npm run server-dev & WEBPACK_DEV=true npm run server",
    "hot": "npm run server-hot & WEBPACK_DEV=true npm run server",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yidea/site-2015"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "example"
  ],
  "author": "email4yi@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/yidea/site-2015",
  "dependencies": {
    "alt": "^0.16.2",
    "autoprefixer-stylus": "^0.7.1",
    "babel": "^5.6.1",
    "babel-core": "^5.6.1",
    "babel-eslint": "^2.0.2",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.6.1",
    "body-parser": "^1.13.2",
    "classnames": "^1.2.2",
    "clean-webpack-plugin": "^0.1.2",
    "compression": "^1.4.3",
    "cookie-parser": "^1.3.5",
    "css-loader": "^0.12.0",
    "es6-promise": "^2.1.1",
    "express": "^4.13.1",
    "express-flash": "0.0.2",
    "express-handlebars": "^2.0.1",
    "express-session": "^1.11.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.1",
    "firebase": "^2.2.7",
    "griddle-react": "^0.2.13",
    "iso": "^4.1.0",
    "isomorphic-fetch": "^2.0.2",
    "json-loader": "^0.5.2",
    "lodash": "^3.9.3",
    "method-override": "^2.3.4",
    "moment": "^2.10.3",
    "morgan": "^1.6.1",
    "node-cache": "^3.0.0",
    "object-assign": "^2.0.0",
    "react": "^0.13.3",
    "react-bootstrap": "^0.21.1",
    "react-router": "^0.13.3",
    "react-tween-state": "0.0.5",
    "style-loader": "^0.8.2",
    "stylus-loader": "^1.2.1",
    "superagent": "^1.2.0",
    "url-loader": "^0.5.5",
    "webpack": "^1.8.11",
    "webpack-stats-plugin": "0.0.4"
  },
  "devDependencies": {
    "eslint": "^0.20.0",
    "eslint-plugin-react": "^2.2.0",
    "http-server": "^0.8.0",
    "marked": "^0.3.3",
    "nodemon": "^1.3.7",
    "react-hot-loader": "^1.2.5",
    "recluster": "^0.4.0",
    "webpack-dev-server": "^1.8.2"
  }
}
